{
    "name": "engalirock/laravel-paymob",
    "description": "Laravel v10 PayMob online payment gateway package",
    "license": "MIT",
    "keywords": [
        "paymob",
        "gateway",
        "online",
        "payment",
        "php",
        "laravel"
    ],
    "homepage": "https://github.com/engalirock/laravel-paymob",
    "authors": [
        {
            "name": "Ali Taha",
            "email": "eng.alirock@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "10.*"
    },
    "autoload": {
        "psr-4": {
            "aliRocK\\PayMob\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "aliRocK\\PayMob\\PayMobServiceProvider"
            ],
            "aliases": {
                "PayMob": "aliRocK\\PayMob\\Facades\\PayMob"
            }
        }
    }
}
